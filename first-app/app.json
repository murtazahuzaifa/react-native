{
  "expo": {
    "name": "first-app",
    "slug": "first-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.murtazahuzaifa.firstapp"
    },
    "web": {
      "favicon": "./app/assets/favicon.png",
      "build": {
        "babel": {
          "include": [
            ""
          ]
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "6adc4580-2525-4c13-9973-2ee22480aa54"
      }
    }
  }
}
